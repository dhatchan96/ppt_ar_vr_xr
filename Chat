from types import SimpleNamespace

@mock.patch('qz.sdlc.rm.services.releasemanager.ReleaseManagerServiceClient.create_release_container')
def test_createReleaseContainer(self, mock_create):
    mock_container = SimpleNamespace(release_key='REL-456')
    mock_create.return_value = (
        200,
        mock_container,
        'OK',
        {}
    )

    rms = ReleaseManagerService('Quartz', 'local')
    self.assertEqual(
        rms.createReleaseContainer('Release-A', 'v1.0', 'JIRA-PROJ', 'Initial deployment'),
        {
            'status': True,
            'status_code': 200,
            'body': 'REL-456'
        }
    )
