jfrom config import OCTANE_CONFIG

def get_octane_url(space_key, workspace_key):
    base_url = OCTANE_CONFIG["base_url"]
    space_id = OCTANE_CONFIG["spaces"][space_key]["id"]
    workspace_id = OCTANE_CONFIG["spaces"][space_key]["workspaces"][workspace_key]
    
    return base_url.format(space_id=space_id, workspace_id=workspace_id)

# Example Usage
url = get_octane_url("space_2", "ws_4")
print(url)  # Output: https://octane.example.com/api/spaces/1002/workspaces/3002/
